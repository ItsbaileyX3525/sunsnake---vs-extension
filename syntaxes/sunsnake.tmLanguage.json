{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "SunSnake",
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#numbers"
		},
		{
			"include": "#reserved-words"
		},
		{
			"include": "#ctrl-statements"
		},
		{
			"captures": {
			"2": {
				"name": "entity.name.function.ss"
			}
			}
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.sunsnake",
				"match": "\\b(if|while|for|return)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.sunsnake",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.sunsnake",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.ss"
}